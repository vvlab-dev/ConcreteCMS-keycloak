{
    "name": "vvlab/keycloak_auth",
    "description": "Let users access your website using Keycloak OpenID servers",
    "type": "concrete5-package",
    "keywords": [
        "concrete5",
        "concretecms",
        "keycloak",
        "openid",
        "authentication"
    ],
    "homepage": "https://github.com/vvlab-dev/ConcreteCMS-keycloak",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Valerio Vinci",
            "email": "dev-support@vvlab.it"
        }
    ],
    "support": {
        "email": "dev-support@vvlab.it",
        "issues": "https://github.com/vvlab-dev/ConcreteCMS-keycloak/issues",
        "source": "https://github.com/vvlab-dev/ConcreteCMS-keycloak"
    },
    "require": {
        "php": "^7.1 || ^8",
        "concrete5/core": "^8.5.12 || ^9"
    },
    "autoload": {
        "psr-4": {
            "vvLab\\KeycloakAuth\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader" : true,
        "preferred-install": "dist",
        "sort-packages" : true,
        "platform": {
            "php": "7.1"
        }
    }
}
